{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential"
  ],
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": ["error", "only-multiline"],
    "space-before-function-paren": ["error", "never"],
    "curly": 0,
    "eqeqeq" :0,
    "vue/multi-word-component-names": "off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}